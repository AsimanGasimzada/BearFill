{
    "manifest_version": 3,
    "name": "BearFill",
    "version": "1.0",
    "description": "Saytlardakı inputları avtomatik doldurur.",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": ["<all_urls>"]
  }
  